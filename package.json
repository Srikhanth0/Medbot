{
  "name": "medbot-project",
  "version": "1.0.0",
  "description": "MedBot - AI Health Assistant with 3D Character Animations",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"cd medbot-app && npm run dev\" \"npm run server\"",
    "server": "node server.js",
    "install:all": "npm install && cd medbot-app && npm install",
    "build": "cd medbot-app && npm run build",
    "start": "npm run server",
    "clean": "cd medbot-app && npm run clean"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "3d",
    "health",
    "medbot",
    "gemini",
    "ai",
    "chatbot",
    "medical-assistant"
  ],
  "author": "MedBot Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
